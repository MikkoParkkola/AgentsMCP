# Logging
AGENTS_LOG_LEVEL=INFO
AGENTS_LOG_FORMAT=json

# Server
AGENTS_SERVER_HOST=0.0.0.0
AGENTS_SERVER_PORT=8000
AGENTS_CORS_ORIGINS=*

# Storage (choose one)
AGENTS_STORAGE_TYPE=memory

# SQLite
AGENTS_SQLITE_PATH=agentsmcp.db

# PostgreSQL
AGENTS_POSTGRES_HOST=localhost
AGENTS_POSTGRES_PORT=5432
AGENTS_POSTGRES_DATABASE=agentsmcp
AGENTS_POSTGRES_USERNAME=postgres
AGENTS_POSTGRES_PASSWORD=postgres

# Redis
AGENTS_REDIS_HOST=localhost
AGENTS_REDIS_PORT=6379

# Observability
AGENTS_PROMETHEUS_ENABLED=true

